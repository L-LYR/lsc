i32[2][4] matMul(i32 a[2][3], i32 b[3][4]) {
    i32 i, j, k;
    i32 ans[2][4];
    for (i = 0; i < 2; ++i) {
        for (j = 0; j < 4; ++j) {
            for (k = 0; k < 3; ++k) {
                ans[i][j] = ans[i][j] + a[i][k] * b[k][j];
            }
        }
    }
    return ans;
}
void matPrint(i32 a[2][4]) {
    i32 i, j;
    for (i = 0; i < 2; ++i) {
        for (j = 0; j < 4; ++j) {
            print("%d", a[i][j]);
            print("%s", " ");
        }
        print("%s", "\n");
    }
    return;
}
i32 fibonacci(i32 n) {
    if (n <= 0) {
        return 0;
    } else if (n == 1) {
        return 1;    
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}
